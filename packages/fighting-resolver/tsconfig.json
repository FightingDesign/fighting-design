{
  "compilerOptions": {
    "target": "esnext",
    "outDir": "../../dist/resolver",
    "useDefineForClassFields": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "declaration": true,
    "lib": [
      "esnext"
    ],
    "skipLibCheck": true
  },
  "include": [
    "index.ts"
  ]
}